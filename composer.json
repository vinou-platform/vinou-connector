{
    "name": "vinou/vinou-connector",
    "type": "typo3-cms-extension",
    "version": "5.0.0-dev",
    "description": "Extension that provides a connection for vinou.de and display wines and winelists via fluid templates",
    "keywords": [
        "TYPO3",
        "extension",
        "vinou"
    ],
    "homepage": "https://www.vinou.de",
    "authors": [{
        "name": "Vinou GmbH",
        "role": "Developer",
        "homepage": "https://www.vinou.de"
    }],
    "license": [
        "GPL-2.0+"
    ],
    "require": {
        "typo3/cms-core": ">=8.7.0,<9.0",
        "facebook/graph-sdk": "*"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "autoload": {
        "psr-4": {
            "Vinou\\VinouConnector\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
