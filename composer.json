{
    "name": "vinou/vinou-connector",
    "type": "typo3-cms-extension",
    "version": "6.0.0",
    "description": "Extension that provides a connection for vinou.de and display wines and winelists via fluid templates",
    "keywords": [
        "TYPO3",
        "extension",
        "vinou"
    ],
    "homepage": "https://www.vinou.de",
    "authors": [{
        "name": "Vinou GmbH",
        "role": "Developer",
        "homepage": "https://www.vinou.de"
    }],
    "license": [
        "GPL-2.0+"
    ],
    "require": {
        "typo3/cms-core": ">=9.5.27,<11.0",
        "vinou/api-connector": "dev-master@dev",
        "vinou/translations": "*"
    },
    "autoload": {
        "psr-4": {
            "Vinou\\VinouConnector\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "vinou_connector",
            "cms-package-dir": "{$vendor-dir}/typo3/cms"
        }
    }
}
